package musicplayer;

import java.awt.*;
import javax.swing.JPanel;

public class MyDrawPanel extends JPanel {
	
	public void paintComponent(Graphics g)
	{
		g.setColor(Color.green);
		g.fillOval(50,50,100,100);  //Fills an oval with the gradient colour.
		
	}

}

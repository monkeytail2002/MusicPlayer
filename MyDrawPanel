package musicplayer;

import java.awt.*;
import javax.swing.JPanel;

public class MyDrawPanel extends JPanel {
	
	int x = 70;
	int y = 70;
	
	public void paintComponent(Graphics g)
	{
		g.setColor(Color.green);  //Sets fill to green
		g.fillOval(x,y,40,40);  //painst and fills oval.
		
	}

}

